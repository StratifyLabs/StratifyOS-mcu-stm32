

MEMORY {
   flash : ORIGIN = 0x08000000, LENGTH = 512K
   ram : ORIGIN = 0x20000000, LENGTH = 112K
   sysmem : ORIGIN = 0x2001C000, LENGTH = 16K
}

SECTIONS {

   .sysmem : {
		_sys = . ;
		*(.sysmem)
		. = ALIGN(1024);
	} >sysmem
	_esys = .;

  . = 0;
  INCLUDE ldscripts/stm32-common.ld
}

PROVIDE ( _end = . );
PROVIDE ( _iap = 0x1FFF1FF1 );
PROVIDE ( _top_of_stack = (0x2001C000 + 16384) );
PROVIDE ( _estack = (0x2001C000 + 16384) );
PROVIDE ( _flash_start = 0x08000000 );
PROVIDE ( _flash_pages = 8 );
PROVIDE ( _flash_size = 524288 );
PROVIDE ( _ram_pages = 112 );
PROVIDE ( _sram_size = 114688 );
PROVIDE ( _sram_start = 0x20000000 );
